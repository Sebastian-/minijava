class TranslateFieldsAndLocals {
	public static void main(String[] a){
	System.out.println(new Driver().start(2));
    }
}

class Driver {
	
	int a;
	int d;
	int[] e;
	
	public int start(int b) {
		int c;
		e = new int[1];
		
		a = 1;
		c = 3;
		d = 0;
		d = d + 4;
		e[1] = 4;
		e[1] = e[1] + 1;
		return a + b + c + d + e[1];
	}
	
}